if game.PlaceId==155615604 then local a=loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()local b=a.CreateLib("Prison Life, By Hadi#7080","Sentinel")local c=b:NewTab("Player")local d=c:NewSection("Player Proporties")d:NewSlider("WalkSpeed","Changes the WalkSpeed",250,16,function(e)game.Players.LocalPlayer.Character.Humanoid.WalkSpeed=e end)d:NewSlider("JumpPower","Changes the WalkSpeed",500,50,function(e)game.Players.LocalPlayer.Character.Humanoid.JumpPower=e end)local f=b:NewTab("Weapons")local g=f:NewSection("Give Gun")g:NewButton("Remington 870","Gives Remington 870",function()local h=game:GetService("Workspace")["Prison_ITEMS"].giver["Remington 870"].ITEMPICKUP;local i=game:GetService("Workspace").Remote.ItemHandler;i:InvokeServer(h)end)g:NewButton("AK-47","Gives Remington 870",function()local h=game:GetService("Workspace")["Prison_ITEMS"].giver["AK-47"].ITEMPICKUP;local i=game:GetService("Workspace").Remote.ItemHandler;i:InvokeServer(h)end)local j=f:NewSection("Mod")j:NewButton("Mod Remington 870","Mod",function()local k=game:GetService("Players").LocalPlayer;local l=k.Backpack:FindFirstChild("Remington 870")or k.Character:FindFirstChild("Remington 870")local m=require(l:FindFirstChild("GunStates"))m["Damage"]=math.huge;m["MaxAmmo"]=math.huge;m["CurrentAmmo"]=math.huge;m["StoredAmmo"]=math.huge;m["FireRate"]=0.1;m["AmmoPerClip"]=math.huge;m["Range"]=math.huge;m["ReloadTime"]=0.000000001;m["Spread"]=0;m["Bullets"]=1;m["AutoFire"]=true end)j:NewButton("Mod AK-47","Mod",function()local k=game:GetService("Players").LocalPlayer;local l=k.Backpack:FindFirstChild("AK-47")or k.Character:FindFirstChild("AK-47")local m=require(l:FindFirstChild("GunStates"))m["Damage"]=math.huge;m["MaxAmmo"]=math.huge;m["CurrentAmmo"]=math.huge;m["StoredAmmo"]=math.huge;m["FireRate"]=0.1;m["AmmoPerClip"]=math.huge;m["Range"]=math.huge;m["ReloadTime"]=0.000000001;m["Spread"]=0;m["Bullets"]=1;m["AutoFire"]=true end)end
